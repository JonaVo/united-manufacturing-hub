{
  "type": "datasource",
  "name": "united-manufacturing-hub-datasource",
  "id": "ia-ia-factoryinsight-datasource",
  "metrics": true,
  "info": {
    "description": "Datasource plugin to retrieve JSON messages from a REST API. Based on the \"simple-json-datasource\" plugin for Grafana and the previous work by Ricardo Vega Ayora.",
    "author": {
      "name": "UMH Systems GmbH",
      "url": "https://www.umh.app"
    },
    "keywords": [
      "JSON",
      "REST",
      "United Manufacturing Hub"
    ],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://github.com/united-manufacturing-hub/united-manufacturing-hub"
      }
    ],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaVersion": "6.5.x",
    "plugins": []
  },
  "routes": [
    {
      "path": "factoryinsight",
      "url": "{{ .JsonData.serverURL }}",
      "headers": [
        {
          "name": "Authorization",
          "content": "{{ .SecureJsonData.apiKey }}"
        },{
          "name": "Content-Type",
          "content": "application/json"
        }
      ]
    }
  ]
}
